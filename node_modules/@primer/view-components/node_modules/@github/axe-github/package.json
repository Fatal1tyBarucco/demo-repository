{
  "name": "@github/axe-github",
  "version": "0.6.1",
  "description": "Custom rules and configuration recommendations for the `axe-core` library for GitHub projects",
  "main": "dist/esm/index.js",
  "types": "dist/types/index.d.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "lint": "eslint src/ test/ types/",
    "test": "npm run karma",
    "test:watch": "karma start test/karma.config.cjs",
    "karma": "karma start test/karma.config.cjs --singleRun",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json && node ./scripts/prepare-package-json && esbuild --bundle src/configure-browser.ts --outfile=dist/configure-browser/configure-browser.js",
    "size-limit": "npm run build && size-limit",
    "prepublishOnly": "npm run build && size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/github/axe-github.git"
  },
  "keywords": [],
  "author": "",
  "prettier": "@github/prettier-config",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/github/axe-github/issues"
  },
  "homepage": "https://github.com/github/axe-github#readme",
  "size-limit": [
    {
      "limit": "150kb",
      "path": "dist/esm/index.js"
    },
    {
      "limit": "150kb",
      "path": "dist/cjs/index.js"
    }
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./configure-browser": {
      "require": "./dist/configure-browser/configure-browser.js"
    }
  },
  "devDependencies": {
    "@github/prettier-config": "0.0.6",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^10.0.1",
    "axe-core": "4.8.1",
    "chai": "^4.3.4",
    "chromium": "^3.0.3",
    "esbuild": "~0.16.17",
    "eslint": "^8.49.0",
    "eslint-plugin-github": "^4.10.0",
    "eslint-plugin-prettier": "^5.0.0",
    "karma": "^6.3.16",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-esbuild": "^2.2.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^10.2.0",
    "prettier": "^3.0.0",
    "size-limit": "^7.0.3",
    "typescript": "^4.4.4"
  }
}
